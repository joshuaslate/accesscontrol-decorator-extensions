{
  "name": "accesscontrol-decorator-extensions",
  "version": "0.0.4",
  "description": "Control access to whole instances or individual properties on those instances with ease.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/test/test-setup.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "node"
    ]
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc"
  },
  "author": "Joshua Anderson Slate",
  "license": "MIT",
  "peerDependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.4",
    "jest": "^23.6.0",
    "reflect-metadata": "^0.1.12",
    "ts-jest": "^23.10.4",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1"
  }
}
